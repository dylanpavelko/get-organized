<h1>Listing event_activity_has_aspects</h1>

<table>
  <thead>
    <tr>
      <th>Event activity</th>
      <th>Planning aspect</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @event_activity_has_aspects.each do |event_activity_has_aspect| %>
      <tr>
        <td><%= event_activity_has_aspect.event_activity %></td>
        <td><%= event_activity_has_aspect.planning_aspect %></td>
        <td><%= link_to 'Show', event_activity_has_aspect %></td>
        <td><%= link_to 'Edit', edit_event_activity_has_aspect_path(event_activity_has_aspect) %></td>
        <td><%= link_to 'Destroy', event_activity_has_aspect, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


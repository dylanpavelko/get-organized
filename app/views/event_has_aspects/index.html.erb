<h1>Listing event_has_aspects</h1>

<table>
  <thead>
    <tr>
      <th>Event</th>
      <th>Planning aspect</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @event_has_aspects.each do |event_has_aspect| %>
      <tr>
        <td><%= event_has_aspect.event %></td>
        <td><%= event_has_aspect.planning_aspect %></td>
        <td><%= link_to 'Show', event_has_aspect %></td>
        <td><%= link_to 'Edit', edit_event_has_aspect_path(event_has_aspect) %></td>
        <td><%= link_to 'Destroy', event_has_aspect, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event has aspect', new_event_has_aspect_path %>

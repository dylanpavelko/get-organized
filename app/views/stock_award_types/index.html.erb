<h1>Listing stock_award_types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stock_award_types.each do |stock_award_type| %>
      <tr>
        <td><%= stock_award_type.name %></td>
        <td><%= link_to 'Show', stock_award_type %></td>
        <td><%= link_to 'Edit', edit_stock_award_type_path(stock_award_type) %></td>
        <td><%= link_to 'Destroy', stock_award_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stock award type', new_stock_award_type_path %>

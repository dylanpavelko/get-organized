<h1>Stocks</h1>

<table>
  <thead>
    <tr>
      <th>Business</th>
      <th>Symbol</th>
    </tr>
  </thead>

  <tbody>
    <% @stocks.each do |stock| %>
      <tr>
        <td><%= link_to stock.business.name, stock %></td>
        <td><%= link_to stock.symbol, stock %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stock', new_stock_path %>

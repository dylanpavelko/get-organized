<h1>Listing user_has_roles</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Role</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_has_roles.each do |user_has_role| %>
      <tr>
        <td><%= user_has_role.user.username %></td>
        <td><%= user_has_role.role.name %></td>
        <td><%= link_to 'Show', user_has_role %></td>
        <td><%= link_to 'Edit', edit_user_has_role_path(user_has_role) %></td>
        <td><%= link_to 'Destroy', user_has_role, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User has role', new_user_has_role_path %>

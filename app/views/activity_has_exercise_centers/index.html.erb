<h1>Listing activity_has_exercise_centers</h1>

<table>
  <thead>
    <tr>
      <th>Activity</th>
      <th>Exercise center</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @activity_has_exercise_centers.each do |activity_has_exercise_center| %>
      <tr>
        <td><%= link_to activity_has_exercise_center.activity.name , activity_has_exercise_center.activity %></td>
        <td><%= link_to activity_has_exercise_center.exercise_center.name , activity_has_exercise_center.exercise_center %></td>
        <td><%= link_to 'Show', activity_has_exercise_center %></td>
        <td><%= link_to 'Edit', edit_activity_has_exercise_center_path(activity_has_exercise_center) %></td>
        <td><%= link_to 'Destroy', activity_has_exercise_center, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Activity has exercise center', new_activity_has_exercise_center_path %>

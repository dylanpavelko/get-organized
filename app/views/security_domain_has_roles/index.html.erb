<h1>Roles for Security Domain</h1>

<table>
  <thead>
    <tr>
      <th>Domain</th>
      <th>Role</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @security_domain_has_roles.each do |security_domain_has_role| %>
      <tr>
        <td><%= link_to security_domain_has_role.domain.name, security_domain_has_role.domain %></td>
        <td><%= link_to security_domain_has_role.role.name, security_domain_has_role.role %></td>
        <td><%= link_to 'Show', security_domain_has_role %></td>
        <td><%= link_to 'Edit', edit_security_domain_has_role_path(security_domain_has_role) %></td>
        <td><%= link_to 'Destroy', security_domain_has_role, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Security domain has role', new_security_domain_has_role_path %>
